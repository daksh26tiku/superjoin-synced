{
  "version": 2,
  "builds": [
    {
      "src": "backend/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/backend/public/dashboard.html"
    },
    {
      "src": "/dashboard",
      "dest": "/backend/public/dashboard.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/backend/public/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/public/$1"
    }
  ]
}
