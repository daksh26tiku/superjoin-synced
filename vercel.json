{
  "version": 2,
  "buildCommand": "node backend/public/build-frontend.js",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
